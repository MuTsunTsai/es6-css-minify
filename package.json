{
    "name": "es6-css-minify",
    "displayName": "ES6 and CSS minifier",
    "description": "Easily minify CSS and JavaScript",
    "version": "0.0.1",
    "publisher": "olback",
    "engines": {
        "vscode": "^1.19.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onLanguage:css",
        "onLanguage:javascript",
        "onCommand:es6-css-minify.minify"
    ],
    "main": "./extension",
    "contributes": {
        "commands": [
            {
                "command": "es6-css-minify.minify",
                "title": "Minify"
            }
        ]
    },
    "scripts": {
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.6",
        "eslint": "^4.6.1",
        "@types/node": "^7.0.43",
        "@types/mocha": "^2.2.42"
    },
    "dependencies": {
        "clean-css": "^4.1.9",
        "uglify-es": "^3.3.8"
    }
}
