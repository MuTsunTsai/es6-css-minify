{
    "name": "es6-css-minify",
    "displayName": "ES5/ES6 and CSS minifier",
    "description": "Easily minify CSS and JavaScript",
    "version": "0.0.1",
    "publisher": "olback",
    "icon": "icon.png",
    "galleryBanner": {
		"color": "#333",
		"theme": "dark"
    },
    "license": "MIT",
    "homepage": "https://github.com/olback/es6-css-minify",
    "bugs": "https://github.com/olback/es6-css-minify/issues",
    "categories": [
        "Other"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/olback/es6-css-minify.git"
    },
    "author": {
		"name": "olback",
		"email": "es6-css-minify@olback.net"
    },
    "keywords": [
        "IDE",
        "vscode",
        "button",
        "simplicity",
        "CSS",
        "js",
        "javascript",
        "minify"
    ],
    "engines": {
        "vscode": "^1.19.0"
    },
    "activationEvents": [
        "onLanguage:css",
        "onLanguage:javascript",
        "onCommand:es6-css-minify.minify"
    ],
    "main": "./extension",
    "contributes": {
        "commands": [
            {
                "command": "es6-css-minify.minify",
                "title": "Minify (ES5/ES6/CSS)"
            }
        ]
    },
    "scripts": {
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.6",
        "eslint": "^4.6.1",
        "@types/node": "^7.0.43",
        "@types/mocha": "^2.2.42"
    },
    "dependencies": {
        "clean-css": "^4.1.9",
        "uglify-es": "^3.3.8"
    }
}
